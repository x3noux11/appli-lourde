{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=MySimpleApiDb;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "Jwt": {
    "Key": "ThisIsMySecretKey12345",
    "Issuer": "MySimpleApi",
    "Audience": "MySimpleApiUsers",
    "ExpiryInMinutes": 60
  },
  "Cors": {
    "AllowedOrigins": ["http://localhost:3000", "https://myapp.example.com"]
  }
}